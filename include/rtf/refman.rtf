81     {\cf18 void} ProcessKeyboard(Movement_Direction direction, GLfloat deltaTime)\par
82     \{\par
83         GLfloat velocity = MovementSpeed * deltaTime;\par
84         {\cf19 if} (direction == FORWARD)\par
85             Position = Position + Front * velocity;\par
86         {\cf19 if} (direction == BACKWARD)\par
87             Position -= Front * velocity;\par
88         {\cf19 if} (direction == LEFT)\par
89             Position -= Right * velocity;\par
90         {\cf19 if} (direction == RIGHT)\par
91             Position += Right * velocity;\par
92     \}\par
