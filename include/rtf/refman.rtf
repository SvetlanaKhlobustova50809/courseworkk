\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid 1 {\cf21 #ifndef SHADER_H}\par
2 {\cf21 #define SHADER_H}\par
3 \par
4 {\cf21 #include <glad/glad.h>}\par
5 {\cf21 #include <glm/glm.hpp>}\par
6 \par
7 {\cf21 #include <string>}\par
8 {\cf21 #include <fstream>}\par
9 {\cf21 #include <sstream>}\par
10 {\cf21 #include <iostream>}\par
11 {\cf21 #include <unordered_map>}\par
12 \par
13 {\cf17 class }Shader\par
14 \{\par
15 {\cf17 public}:\par
16     {\cf18 unsigned} {\cf18 int} ID;\par
17     {\cf20 // \'EA\'EE\'ED\'F1\'F2\'F0\'F3\'EA\'F2\'EE\'F0 \'E3\'E5\'ED\'E5\'F0\'E8\'F0\'F3\'E5\'F2 \'F8\'E5\'E9\'E4\'E5\'F0}\par
18     Shader({\cf17 const} {\cf18 char}* vertexPath, {\cf17 const} {\cf18 char}* fragmentPath)\par
19     \{\par
20         {\cf20 // 1. retrieve the vertex/fragment source code from filePath}\par
21         std::string vertexCode;\par
22         std::string fragmentCode;\par
23         std::ifstream vShaderFile;\par
24         std::ifstream fShaderFile;\par
25         {\cf20 // ensure ifstream objects can throw exceptions:}\par
26         vShaderFile.exceptions(std::ifstream::failbit | std::ifstream::badbit);\par
27         fShaderFile.exceptions(std::ifstream::failbit | std::ifstream::badbit);\par
28         {\cf19 try}\par
29         \{\par
30             {\cf20 // open files}\par
31             vShaderFile.open(vertexPath);\par
32             fShaderFile.open(fragmentPath);\par
33             std::stringstream vShaderStream, fShaderStream;\par
34             {\cf20 // read file's buffer contents into streams}\par
35             vShaderStream << vShaderFile.rdbuf();\par
36             fShaderStream << fShaderFile.rdbuf();\par
37             {\cf20 // close file handlers}\par
38             vShaderFile.close();\par
39             fShaderFile.close();\par
40             {\cf20 // convert stream into string}\par
41             vertexCode = vShaderStream.str();\par
42             fragmentCode = fShaderStream.str();\par
43         \}\par
44         {\cf19 catch} (std::ifstream::failure& e)\par
45         \{\par
46             std::cout << {\cf22 "ERROR::SHADER::FILE_NOT_SUCCESFULLY_READ: "} << e.what() << std::endl;\par
47         \}\par
48         {\cf17 const} {\cf18 char}* vShaderCode = vertexCode.c_str();\par
49         {\cf17 const} {\cf18 char}* fShaderCode = fragmentCode.c_str();\par
50         {\cf20 // 2. compile shaders}\par
51         {\cf18 unsigned} {\cf18 int} vertex, fragment;\par
52         {\cf20 // vertex shader}\par
53         vertex = glCreateShader(GL_VERTEX_SHADER);\par
54         glShaderSource(vertex, 1, &vShaderCode, NULL);\par
55         glCompileShader(vertex);\par
56         checkCompileErrors(vertex, {\cf22 "VERTEX"});\par
57         {\cf20 // fragment Shader}\par
58         fragment = glCreateShader(GL_FRAGMENT_SHADER);\par
59         glShaderSource(fragment, 1, &fShaderCode, NULL);\par
60         glCompileShader(fragment);\par
61         checkCompileErrors(fragment, {\cf22 "FRAGMENT"});\par
62         {\cf20 // shader Program}\par
63         ID = glCreateProgram();\par
64         glAttachShader(ID, vertex);\par
65         glAttachShader(ID, fragment);\par
66         glLinkProgram(ID);\par
67         checkCompileErrors(ID, {\cf22 "PROGRAM"});\par
68         {\cf20 // delete the shaders as they're linked into our program now and no longer necessery}\par
69         glDeleteShader(vertex);\par
70         glDeleteShader(fragment);\par
71 \par
72     \}\par
73     {\cf20 // activate the shader}\par
74     {\cf18 void} use(){\cf17  const}\par
75 {\cf17     }\{\par
76         glUseProgram(ID);\par
77     \}\par
78 \par
79      {\cf18 void} setBool({\cf17 const} std::string& name, {\cf18 bool} value){\cf17  const}\par
80 {\cf17     }\{\par
81         glUniform1i(glGetUniformLocation(ID, name.c_str()), ({\cf18 int})value);\par
82     \}\par
83 \par
84     {\cf18 void} setInt({\cf17 const} std::string& name, {\cf18 int} value){\cf17  const}\par
85 {\cf17     }\{\par
86         glUniform1i(glGetUniformLocation(ID, name.c_str()), value);\par
87     \}\par
88     \par
89     {\cf18 void} setFloat({\cf17 const} std::string& name, {\cf18 float} value){\cf17  const}\par
90 {\cf17     }\{\par
91         glUniform1f(glGetUniformLocation(ID, name.c_str()), value);\par
92     \}\par
93     \par
94     {\cf18 void} setVec2({\cf17 const} std::string& name, {\cf17 const} glm::vec2& value){\cf17  const}\par
95 {\cf17     }\{\par
96         glUniform2fv(glGetUniformLocation(ID, name.c_str()), 1, &value[0]);\par
97     \}\par
98     {\cf18 void} setVec2({\cf17 const} std::string& name, {\cf18 float} x, {\cf18 float} y){\cf17  const}\par
99 {\cf17     }\{\par
100         glUniform2f(glGetUniformLocation(ID, name.c_str()), x, y);\par
101     \}\par
102     \par
103     {\cf18 void} setVec3({\cf17 const} std::string& name, {\cf17 const} glm::vec3& value){\cf17  const}\par
104 {\cf17     }\{\par
105         glUniform3fv(glGetUniformLocation(ID, name.c_str()), 1, &value[0]);\par
106     \}\par
107 \par
108     {\cf18 void} setVec3({\cf17 const} std::string& name, {\cf18 float} x, {\cf18 float} y, {\cf18 float} z){\cf17  const}\par
109 {\cf17     }\{\par
110         glUniform3f(glGetUniformLocation(ID, name.c_str()), x, y, z);\par
111     \}\par
112     \par
113     {\cf18 void} setVec4({\cf17 const} std::string& name, {\cf17 const} glm::vec4& value){\cf17  const}\par
114 {\cf17     }\{\par
115         glUniform4fv(glGetUniformLocation(ID, name.c_str()), 1, &value[0]);\par
116     \}\par
117 \par
118     {\cf18 void} setVec4({\cf17 const} std::string& name, {\cf18 float} x, {\cf18 float} y, {\cf18 float} z, {\cf18 float} w){\cf17  const}\par
119 {\cf17     }\{\par
120         glUniform4f(glGetUniformLocation(ID, name.c_str()), x, y, z, w);\par
121     \}\par
122     \par
123     {\cf18 void} setMat2({\cf17 const} std::string& name, {\cf17 const} glm::mat2& mat){\cf17  const}\par
124 {\cf17     }\{\par
125         glUniformMatrix2fv(glGetUniformLocation(ID, name.c_str()), 1, GL_FALSE, &mat[0][0]);\par
126     \}\par
127     \par
128     {\cf18 void} setMat3({\cf17 const} std::string& name, {\cf17 const} glm::mat3& mat){\cf17  const}\par
129 {\cf17     }\{\par
130         glUniformMatrix3fv(glGetUniformLocation(ID, name.c_str()), 1, GL_FALSE, &mat[0][0]);\par
131     \}\par
132     \par
133     {\cf18 void} setMat4({\cf17 const} std::string& name, {\cf17 const} glm::mat4& mat){\cf17  const}\par
134 {\cf17     }\{\par
135         glUniformMatrix4fv(glGetUniformLocation(ID, name.c_str()), 1, GL_FALSE, &mat[0][0]);\par
136     \}\par
137 \par
138 {\cf17 private}:\par
139     {\cf20 // \'F1\'EB\'F3\'E6\'E5\'E1\'ED\'E0\'FF \'F4\'F3\'ED\'EA\'F6\'E8\'FF \'E4\'EB\'FF \'EF\'F0\'EE\'E2\'E5\'F0\'EA\'E8 \'EE\'F8\'E8\'E1\'EE\'EA \'EA\'EE\'EC\'EF\'E8\'EB\'FF\'F6\'E8\'E8/\'EA\'EE\'EC\'EF\'EE\'ED\'EE\'E2\'EA\'E8 \'F8\'E5\'E9\'E4\'E5\'F0\'EE\'E2.}\par
140     {\cf18 void} checkCompileErrors(GLuint shader, std::string type)\par
141     \{\par
142         GLint success;\par
143         GLchar infoLog[1024];\par
144         {\cf19 if} (type != {\cf22 "PROGRAM"})\par
145         \{\par
146             glGetShaderiv(shader, GL_COMPILE_STATUS, &success);\par
147             {\cf19 if} (!success)\par
148             \{\par
149                 glGetShaderInfoLog(shader, 1024, NULL, infoLog);\par
150                 std::cout << {\cf22 "ERROR::SHADER_COMPILATION_ERROR of type: "} << type << {\cf22 "\\n"} << infoLog << {\cf22 "\\n"} << std::endl;\par
151             \}\par
152         \}\par
153         {\cf19 else}\par
154         \{\par
155             glGetProgramiv(shader, GL_LINK_STATUS, &success);\par
156             {\cf19 if} (!success)\par
157             \{\par
158                 glGetProgramInfoLog(shader, 1024, NULL, infoLog);\par
159                 std::cout << {\cf22 "ERROR::PROGRAM_LINKING_ERROR of type: "} << type << {\cf22 "\\n"} << infoLog << {\cf22 "\\n"} << std::endl;\par
160             \}\par
161         \}\par
162     \}\par
163 \};\par
164 \par
165 {\cf21 #endif}\par
}

