134     {\cf18 void} updateCameraVectors()\par
135     \{\par
136         {\cf20 // \'E2\'FB\'F7\'E8\'F1\'EB\'E8\'F2\'E5 \'ED\'EE\'E2\'FB\'E9 \'E2\'E5\'EA\'F2\'EE\'F0 \'F4\'F0\'EE\'ED\'F2\'E0}\par
137         glm::vec3 front;\par
138         front.x = cos(glm::radians(Yaw)) * cos(glm::radians(Pitch));\par
139         front.y = sin(glm::radians(Pitch));\par
140         front.z = sin(glm::radians(Yaw)) * cos(glm::radians(Pitch));\par
141         Front = glm::normalize(front);\par
142         {\cf20 // \'F2\'E0\'EA\'E6\'E5 \'EF\'E5\'F0\'E5\'F1\'F7\'E8\'F2\'E0\'E9\'F2\'E5 \'E2\'E5\'EA\'F2\'EE\'F0 \'E2\'EF\'F0\'E0\'E2\'EE \'E8 \'E2\'E2\'E5\'F0\'F5}\par
143         Right = glm::normalize(glm::cross(Front, WorldUp));  {\cf20 // \'ED\'EE\'F0\'EC\'E0\'EB\'E8\'E7\'F3\'E9\'F2\'E5 \'E2\'E5\'EA\'F2\'EE\'F0\'FB, \'EF\'EE\'F2\'EE\'EC\'F3 \'F7\'F2\'EE \'E8\'F5 \'E4\'EB\'E8\'ED\'E0 \'F1\'F2\'E0\'ED\'EE\'E2\'E8\'F2\'F1\'FF \'E1\'EB\'E8\'E6\'E5 \'EA 0, \'F7\'E5\'EC \'E1\'EE\'EB\'FC\'F8\'E5 \'E2\'FB \'F1\'EC\'EE\'F2\'F0\'E8\'F2\'E5 \'E2\'E2\'E5\'F0\'F5 \'E8\'EB\'E8 \'E2\'ED\'E8\'E7, \'F7\'F2\'EE \'EF\'F0\'E8\'E2\'EE\'E4\'E8\'F2 \'EA \'E7\'E0\'EC\'E5\'E4\'EB\'E5\'ED\'E8\'FE \'E4\'E2\'E8\'E6\'E5\'ED\'E8\'FF.}\par
144         Up = glm::normalize(glm::cross(Right, Front));\par
145     \}\par
146 \};\par
147 \par
148 {\cf21 #endif}\par
}
