29 {\cf17 class }Camera\par
30 \{\par
31 {\cf17 public}:\par
32     {\cf20 // \'E0\'F2\'F0\'E8\'E1\'F3\'F2\'FB \'EA\'E0\'EC\'E5\'F0\'FB}\par
33     glm::vec3 Position;\par
34     glm::vec3 Front;\par
35     glm::vec3 Up;\par
36     glm::vec3 Right;\par
37     glm::vec3 WorldUp;\par
38     {\cf20 // \'F3\'E3\'EB\'FB \'DD\'E9\'EB\'E5\'F0\'E0}\par
39     GLfloat Yaw;\par
40     GLfloat Pitch;\par
41     {\cf20 // \'EF\'E0\'F0\'E0\'EC\'E5\'F2\'F0\'FB \'EA\'E0\'EC\'E5\'F0\'FB}\par
42     GLfloat MovementSpeed;\par
